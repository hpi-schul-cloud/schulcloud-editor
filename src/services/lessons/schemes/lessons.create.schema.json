{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"title": "LessonCreate",
	"required": [
		"courseId",
		"createdBy"
	],
	"properties": {
    	"title": {
    		"$id": "#/properties/title",
    		"type": "string"
    	},
		"visible": {
			"$id": "#/properties/visible",
 			"type": "boolean"
		},
		"position": {
			"$id": "#/properties/position",
			"type": "number"
		},
		"courseId": {
			"$id": "#/properties/courseId",
			"type": "string",
			"pattern": "[a-f0-9]{24}"
		},
		"createdBy": {
			"$id": "#/properties/createdBy",
			"type": "string",
			"pattern": "[a-f0-9]{24}"
		},
		"fork": {
			"$id": "#/properties/fork",
			"type": "string",
			"pattern": "[a-f0-9]{24}"
		},
		"hash": {
			"$id": "#/properties/hash",
			"type": "string",
			"pattern": "[a-f0-9]{24}"
		}
	},
	"additionalProperties": false
}